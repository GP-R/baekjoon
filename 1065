#include<iostream>
#include<cstring>
bool check_hansu(int n)
{
	if (n == 0)
		return false;
	if (n < 100)
		return true;
	char arr[10];
	int d[10];
	int count = 0;
	sprintf(arr, "%d", n);
	for (int i = 0; i < strlen(arr)-1; i++)
	{
		d[i] = arr[i] - arr[i + 1];//135 -> d[0] = 2 ,d[1] = 2
	}
	for (int i = 0; i < strlen(arr) - 2; i++)
	{
		if (d[i] == d[i + 1])
			count++;
		else
			return false;
	}
	if (count == strlen(arr) - 2)
		return true;
	return false;
}

int main()
{
	int n;
	scanf("%d", &n);
	int ans = 0;
	bool result[1001] = { false };
	for (int i = 1; i <= n; i++)
	{
		result[i] = check_hansu(i); //0~99 -> 100ê°œ
		if (result[i] == true)
			ans++;
	}
	printf("%d", ans);
	return 0;
}
